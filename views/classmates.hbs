  <div class="container">
    <h2 class="center orange-text">Teachers</h2>
    <ul class="collection ">
{{#each users}}
{{#ifCond role '==' 'teacher'}}
 <li class="collection-item avatar">
        <i class="material-icons circle"> 
            <img src="{{imageUrl}}" width="40px" height="40px" >
       </i>
        <span class="title">{{name}}</span>
        <p class="grey-text">{{email}}</p>
        <a href="/private/{{name}}" class="secondary-content orange-text">
          <span>send msg <i class="material-icons ">create</i></span>
        </a>
      </li>
{{/ifCond}}
{{/each}}
    </ul>
       <h2 class="center orange-text">Classmates</h2>
    <ul class="collection ">
{{#each users}}
{{#ifCond role '==' 'student'}}
 <li class="collection-item avatar">
        <i class="material-icons circle"> 
            <img src="{{imageUrl}}" width="40px" height="40px" >
       </i>
        <span class="title">{{name}}</span>
        <p class="grey-text">{{email}}</p>
        <a href="/private/{{name}}" class="secondary-content orange-text">
          <span>send msg <i class="material-icons ">create</i></span>
        </a>
      </li>
{{/ifCond}}
{{/each}}
    </ul>
  </div>

  <!-- Compiled and minified JavaScript -->
